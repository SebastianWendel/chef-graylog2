production:
  host: <%= node['graylog2']['mongo_host'] %>
  port: <%= node['graylog2']['mongo_port'] %>
<% if node['graylog2']['MONGO_AUTH'] == true %>                                                                           
  username: <%= node['graylog2']['mongo_user'] %>
  password: <%= node['graylog2']['mongo_passwd'] %>
<% else %>
  username:
  password:
  database: <%= node['graylog2']['mongo_database'] %>
<% end %>
